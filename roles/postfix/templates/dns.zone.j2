;Zone file for c31348.sa.cs.ut.ee
$TTL 	15M
@ 	IN 	SOA ns1.{{hostname}}.{{domain_name}} root.{{hostname}}.{{domain_name}} (
	2012020204	;Serial
	15M	;Refresh
	5M	;Retry
	120M	;Expire
	600 )	;Negative Cache TTL)
@ 	IN 	NS	ns1
@ 	IN 	A {{ip_address}}
ns1 	IN 	A {{ip_address}}
{{hostname}} 	IN 	A {{ip_address}}
mail	IN 	A {{ip_address}}
www 	IN	CNAME {{hostname}}.{{domain_name}}.
proxy 	IN 	CNAME {{hostname}}.{{domain_name}}.
wordpress IN 	CNAME {{hostname}}.{{domain_name}}.
{{hostname}}.{{domain_name}}. IN MX 10 mail.{{hostname}}.{{domain_name}}.
